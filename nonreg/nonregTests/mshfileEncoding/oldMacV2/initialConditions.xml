<?xml version = "1.0" encoding = "UTF-8" standalone = "yes"?>
<CI>
    <!-- LIST OF GEOMETRICAL DOMAINS  -->
    <physicalDomains>
        <domain name="base" state="air" type="entireDomain"/>
    </physicalDomains>

    <!-- LIST OF BOUNDARY CONDITIONS -->
    <boundaryConditions>
        <boundCond name="inlet" type="inletInjStagState" number="1">
+         <dataInletInj m0="50."/>
          <dataFluid EOS="IG_air.xml" density="1.0" pressure="1.e5"/>
        </boundCond>
        <boundCond name="outlet" type="nonReflecting" number="2"/>

    </boundaryConditions>

    <!--  LIST OF STATES  -->
    <state name="air">
        <material type="fluid" EOS="IG_air.xml">
            <dataFluid density="1.0" pressure="1.e5">
                <velocity x="0." y="0." z="0."/>
            </dataFluid>
        </material>
    </state>

</CI>
