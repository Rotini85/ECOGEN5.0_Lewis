<?xml version = "1.0" encoding = "UTF-8" standalone = "yes"?>
<CI>
    <!-- LIST OF GEOMETRICAL DOMAINS  -->
    <physicalDomains>
        <domain name="base" state="air" type="entireDomain"/>
        <domain name="cylinder"  state="waterCylinder" type="cylinder">
            <dataCylinder axis1="x" axis2="y" radius="0.2" length="1.">
                <center x="0.5" y="0.5" z="0."/>
            </dataCylinder>
        </domain>
    </physicalDomains>

    <!-- LIST OF BOUNDARY CONDITIONS -->
    <boundaryConditions>
        <boundCond name="BC_Xmin" type="nonReflecting" number="1"/>
        <boundCond name="BC_Xmax" type="nonReflecting" number="2"/>
        <boundCond name="BC_Ymin" type="nonReflecting" number="3"/>
        <boundCond name="BC_Ymax" type="nonReflecting" number="4"/>
        <boundCond name="BC_Zmin" type="wall" number="5"/>
        <boundCond name="BC_Zmax" type="wall" number="6"/>
    </boundaryConditions>

    <!--  LIST OF STATES  -->
    <state name="air">
        <material type="fluid" EOS="IG_air.xml">
            <dataFluid alpha="0.999999" density="1.2"/>
        </material>
        <material type="fluid" EOS="SG_water.xml">
            <dataFluid alpha="0.000001" density="950.0"/>
        </material>
        <mixture>
            <dataMix pressure = "1.e5"/>
            <velocity x="0." y="0." z="0."/>
        </mixture>
    </state>

    <state name="waterCylinder">
        <material type="fluid" EOS="IG_air.xml">
            <dataFluid alpha="0.000001" density="2.4"/>
        </material>
        <material type="fluid" EOS="SG_water.xml">
            <dataFluid alpha="0.999999" density="1000.0"/>
        </material>
        <mixture>
            <dataMix pressure = "1.e5"/>
            <velocity x="0." y="0." z="0."/>
        </mixture>
    </state>

</CI>
