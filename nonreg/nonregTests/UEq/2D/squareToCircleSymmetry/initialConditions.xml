<CI>
    <!-- LIST OF GEOMETRICAL DOMAINS  -->
    <physicalDomains>
        <domain name="base" state="air" type="entireDomain"/>
        <domain name="square"  state="square" type="cuboid">
            <dataCuboid lAxisX="0.1" lAxisY="0.1" lAxisZ="1.">
                <posInferiorVertex x="0." y="0." z="0."/>
            </dataCuboid>
        </domain>
    </physicalDomains>

