<?xml version = "1.0" encoding = "UTF-8" standalone = "yes"?>
<computationParam>
	<run>euler3DshockTubeCylinder</run>
	<outputMode format="VTK" binary="false"/>
	<timeControlMode iterations="false">
		<iterations number="1" iterFreq="1"/>
		<physicalTime totalTime="1.e-4" timeFreq="1.e-5"/>
	</timeControlMode>
	<computationControl CFL="0.6"/>

  <gradient>
    <method>green-gauss</method>
  </gradient>

  <secondOrder>
		<globalLimiter>minmod</globalLimiter>
	</secondOrder>
</computationParam>
