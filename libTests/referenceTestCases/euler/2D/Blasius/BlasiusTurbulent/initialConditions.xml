<?xml version = "1.0" encoding = "UTF-8" standalone = "yes"?>
<CI>
    <!-- LIST OF GEOMETRICAL DOMAINS  -->
    <physicalDomains>
        <domain name="base" state="air" type="entireDomain"/>
        <!-- <domain name="base" state="air" type="entireDomain" physicalEntity="10"/> -->
    </physicalDomains>

    <!-- LIST OF BOUNDARY CONDITIONS -->
    <boundaryConditions>
        <!-- Requires hard-coded symmetry boundary condition before the flat plate, see MeshCartesian.cpp -->
        <boundCond name="BC_Xmin" type="inletInjStagState" number="1">
            <dataInletInj m0="122.4"/>
            <dataFluid EOS="IG_airBlasiusTurbulent.xml" density="1.2" pressure="1.e5"/>
        </boundCond>
        <boundCond name="BC_Xmax" type="outletPressure" number="2">
            <dataOutletPressure p0="1.e5"/>
        </boundCond>
        <boundCond name="BC_Ymin" type="wall" number="3"/>
        <boundCond name="BC_Ymax" type="outletPressure" number="4">
            <dataOutletPressure p0="1.e5"/>
        </boundCond>
    </boundaryConditions>

    <!-- Unstructured mesh -->
    <!-- <boundaryConditions> -->
        <!-- <boundCond name="BC_Xmin" type="nonReflecting" number="1"/> -->
        <!-- <boundCond name="BC_Xmax" type="nonReflecting" number="2"/> -->
        <!-- <boundCond name="BC_Ymax" type="nonReflecting" number="3"/> -->
        <!-- <boundCond name="BC_YminL" type="symmetry" number="4"/> -->
        <!-- <boundCond name="BC_YminR" type="wall" number="5"/> -->
    <!-- </boundaryConditions> -->

    <!--  LIST OF STATES  -->
    <state name="air">
        <material type="fluid" EOS="IG_airBlasiusTurbulent.xml">
            <dataFluid density="1.2" pressure="1.e5">
                <velocity x="0." y="0." z="0."/>
            </dataFluid>
        </material>
    </state>

</CI>
