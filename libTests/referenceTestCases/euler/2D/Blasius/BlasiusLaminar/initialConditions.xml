<?xml version = "1.0" encoding = "UTF-8" standalone = "yes"?>
<CI>
    <!-- LIST OF GEOMETRICAL DOMAINS  -->
    <physicalDomains>
        <domain name="base" state="air" type="entireDomain"/>
    </physicalDomains>

    <!-- LIST OF BOUNDARY CONDITIONS -->
    <boundaryConditions>
        <boundCond name="BC_Xmin" type="inletInjStagState" number="1">
            <dataInletInj m0="1."/>
            <dataFluid EOS="IG_airBlasiusLaminar.xml" density="1." pressure="100."/>
        </boundCond>

        <boundCond name="BC_Xmax" type="outletPressure" number="2">
            <dataOutletPressure p0="100."/>
        </boundCond>

        <boundCond name="BC_Ymin" type="wall" number="3"/>

        <boundCond name="BC_Ymax" type="outletPressure" number="4">
            <dataOutletPressure p0="100."/>
        </boundCond>

    </boundaryConditions>

    <!--  LIST OF STATES  -->
    <state name="air">
        <material type="fluid" EOS="IG_airBlasiusLaminar.xml">
            <dataFluid density="1." pressure="100.">
                <velocity x="0." y="0." z="0."/>
            </dataFluid>
        </material>
    </state>

</CI>
