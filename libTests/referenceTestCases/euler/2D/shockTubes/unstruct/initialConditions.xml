                <velocity x="0." y="0." z="0."/>
            </dataFluid>
        </material>
        <transport name="transport" value="1."/>
    </state>

    <state name="rightChamber">
        <material type="fluid" EOS="IG_air.xml">
            <dataFluid density="1.0" pressure="1.e5">
                <velocity x="0." y="0." z="0."/>
            </dataFluid>
        </material>
        <transport name="transport" value="0."/>
    </state>

</CI>
